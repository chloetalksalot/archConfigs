{
   "author": "Nicobelic",
   "browser_action": {
      "default_icon": "img/iconStore.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "js": [ "js/content.js", "js/jquery.min.js", "js/popup.js" ],
      "matches": [ "*://thepiratebay.org/*", "*://www.thepiratebay.org/*", "*://pirateproxy.cam/*" ]
   } ],
   "description": "This extention gives you the ability to hide fake torrents on TPB, as well as other fun features.",
   "icons": {
      "128": "img/iconStore.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Hide Fake Torrents on TPB",
   "permissions": [ "activeTab", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.4"
}
